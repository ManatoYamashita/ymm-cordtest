{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["@next/eslint-plugin-next", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "indent": ["error", 2],
    "no-console": "warn"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
